        else
            if (d<0)
               cout<<"La solucion es imaginaria"<<endl;
        else
        {
            x1=(-b+sqrt(b*b-4*a*c))/(2*a);
            x2=(-b-sqrt(b*b-4*a*c))/(2*a);
